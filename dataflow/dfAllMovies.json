{
	"name": "dfAllMovies",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DSSource",
						"type": "DatasetReference"
					},
					"name": "srcMoviesDB"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "SinkAllDS",
						"type": "DatasetReference"
					},
					"name": "SinkAllDS"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          movie as string,",
				"          title as string,",
				"          genres as string,",
				"          year as string,",
				"          Rating as string,",
				"          {Rotton Tomato} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> srcMoviesDB",
				"srcMoviesDB sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> SinkAllDS"
			]
		}
	}
}